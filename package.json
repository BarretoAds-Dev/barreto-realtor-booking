{
  "name": "barreto-realtor-booking",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "pnpm run build && pnpm run github && wrangler deploy",
    "deploy:preview": "pnpm run build && wrangler deploy --env preview",
    "cf:dev": "pnpm run build && wrangler dev",
    "github": "git add . && git commit -m \"Deploy: $(date +'%Y-%m-%d %H:%M:%S')\" && git push origin HEAD",
    "test:apis": "node scripts/test-apis.js",
    "test:apis:sh": "bash scripts/test-apis.sh",
    "health-check": "node scripts/health-check.js"
  },
  "dependencies": {
    "@astrojs/preact": "^4.1.3",
    "@preact/signals": "^2.5.1",
    "@supabase/supabase-js": "^2.86.0",
    "astro": "^5.16.3",
    "preact": "^10.27.2",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/tailwind": "^6.0.2",
    "@types/node": "^24.10.1",
    "tailwindcss": "^3.4.18",
    "wrangler": "^3.78.0"
  }
}
